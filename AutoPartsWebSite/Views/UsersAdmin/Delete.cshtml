@model IdentityAutoPart.Models.ApplicationUser

@{
    ViewBag.Title = "УДАЛЕНИЕ ПОЛЬЗОВАТЕЛЯ";
}
<DIV class="main-text">
<h1 id="cntr">@ViewBag.Title</h1>
<div class="reg-form">
<br>

<h3>@Html.DisplayFor(model => model.FirstName) @Html.DisplayFor(model => model.LastName)</h3>
<p>E-MAIL: @Html.DisplayFor(model => model.UserName)</p>	
<p>ТЕЛ: @Html.DisplayFor(model => model.Phone)</p>
<p>ЛИМИТ: $@Html.DisplayFor(model => model.MoneyLimit)</p>
<p>ПОИСК ПОЗИЦИЙ: @Html.DisplayFor(model => model.SearchLimit)</p>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

            <input type="submit" value="УДАЛИТЬ" class="btn btn-default" />
    }
</div>
<br>
<div id="cntr">@Html.ActionLink("Назад", "Index")</div>
</div>
